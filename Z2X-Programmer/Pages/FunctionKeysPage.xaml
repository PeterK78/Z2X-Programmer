<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Z2XProgrammer.Pages.FunctionKeysPage"
             xmlns:converter="clr-namespace:Z2XProgrammer.Converter"
             xmlns:strings="clr-namespace:Z2XProgrammer.Resources.Strings"
             xmlns:viewmodel="clr-namespace:Z2XProgrammer.ViewModel"
             xmlns:datamodel="clr-namespace:Z2XProgrammer.DataModel"
             x:DataType="viewmodel:FunctionKeysViewModel"
             BackgroundColor="{AppThemeBinding Light={StaticResource AppBackground_Light}, Dark={StaticResource AppBackground_Dark}}"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             Padding="40,40,40,40"
             Title="{x:Static strings:AppResources.FlyoutItemFunctionKeysTitle}">


    <ContentPage.Resources>
        <converter:InverseBooleanConverter x:Key="boolInvert" />
        <converter:ZIMOInputMappingConverter x:Key="ZIMOInputMappingConverter" />
        <converter:SwitchStateToTextConverter x:Key="switchStateConverter" />
    </ContentPage.Resources>

    <ScrollView x:Name="PageScrollView" Orientation="Vertical" Padding="0,0,20,0">
        
        <VerticalStackLayout>

            <!-- Page Title -->
            <HorizontalStackLayout>
                <Image Source="{AppThemeBinding Light=ic_fluent_keyboard_123_24_regular.png, Dark=ic_fluent_keyboard_123_24_dark.png}" Margin="0,0,10,0" ></Image>
                <Label Style="{StaticResource Z2XProgrammerLabelHeading1Style}" Text="{x:Static strings:AppResources.FlyoutItemFunctionKeysTitle}" VerticalOptions="Center"  HorizontalOptions="Start" />
            </HorizontalStackLayout>

            <!-- No data available information -->
            <HorizontalStackLayout IsVisible="{Binding Path=DataStoreDataValid,Converter={StaticResource boolInvert}}" Margin="0,20,0,20" >
                <Image Source="{AppThemeBinding Light=ic_fluent_info_24_regular.png, Dark=ic_fluent_info_24_dark.png}" WidthRequest="24" Margin="0,0,20,0" ></Image>
                <Label Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.AlertNoInformationAvailable}" VerticalOptions="Center"  HorizontalOptions="Start" />
            </HorizontalStackLayout>

            <!-- Page sheet content -->
            <VerticalStackLayout IsVisible="{Binding DataStoreDataValid}">

                <!-- ZIMO_INPUTMAPPING_CV4XX -->
                <Label Style="{StaticResource Z2XProgrammerLabelHeading2Style}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingTitle}" VerticalOptions="Center"  HorizontalOptions="Start" IsVisible="{Binding ZIMO_INPUTMAPPING_CV4XX}" />
                <Frame Style="{StaticResource Z2XProgrammerFrame}" IsVisible="{Binding ZIMO_INPUTMAPPING_CV4XX}">
                    <VerticalStackLayout>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                                <RowDefinition Height="Auto"></RowDefinition>
                            </Grid.RowDefinitions>

                            <Label x:Name="FrameFunctionKeysZIMOInputMappingDescription" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingDescription}" VerticalOptions="Center"  HorizontalOptions="Start" />

                            <CollectionView Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" ItemsSource="{Binding ZIMOInputMappingCVs}" SelectedItem="{Binding SelectedInputMapping}" HeightRequest="300" Margin="0,20,0,0" SelectionMode="Single">
                                <CollectionView.Header>
                                    <Grid ColumnDefinitions="*,*,*">
                                        <Label Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalTextAlignment="Center" HorizontalTextAlignment="Center"  BackgroundColor="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingHeaderTitleController}" Grid.Column="0"/>
                                        <Label Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalTextAlignment="Center" HorizontalTextAlignment="Center"  BackgroundColor="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingHeaderTitleCV}" Grid.Column="1"/>
                                        <Label Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalTextAlignment="Center" HorizontalTextAlignment="Center" BackgroundColor="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingHeaderTitleDecoder}" Grid.Column="2"/>
                                    </Grid>
                                </CollectionView.Header>

                                <CollectionView.ItemTemplate>
                                    <DataTemplate x:DataType="datamodel:ZIMOInputMappingType">
                                        <Grid ColumnDefinitions="*,*,*">
                                            <Label Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{Binding ExternalFunctionKeyDescription}" HorizontalTextAlignment="Start" />
                                            <Label Grid.Column="1" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{Binding CVNumber}" HorizontalTextAlignment="Center" />
                                            <Label Grid.Column="2" Style="{StaticResource Z2XProgrammerLabelStandardStyle}"  HorizontalTextAlignment="Start" Text="{Binding Path=InternalFunctionKeyNumber,Converter={StaticResource ZIMOInputMappingConverter}}"/>
                                        </Grid>
                                    </DataTemplate>

                                </CollectionView.ItemTemplate>
                            </CollectionView>

                            <Button Margin="0,20,0,0" Grid.Row="3" Grid.Column="0" WidthRequest="200" HorizontalOptions="End" Style="{StaticResource Z2XProgrammerStandardButtonStyle}" Text="{x:Static strings:AppResources.FrameFunctionKeysZIMOInputMappingEdit}" CommandParameter="{Binding .}" Command="{Binding Source={RelativeSource AncestorType={x:Type viewmodel:FunctionKeysViewModel}}, Path=EditInputMappingCommand}"  ></Button>

                        </Grid>
                    </VerticalStackLayout>
                </Frame>

                <!-- RCN225 standard function mapping in CVs #33 to #46  -->
                <Label Style="{StaticResource Z2XProgrammerLabelHeading2Style}" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingTitle}" VerticalOptions="Center"  HorizontalOptions="Start" IsVisible="{Binding RCN225_FUNCTIONKEYMAPPING_CV3346}" />
                <Frame Style="{StaticResource Z2XProgrammerFrame}" IsVisible="{Binding RCN225_FUNCTIONKEYMAPPING_CV3346}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            <RowDefinition Height="*"></RowDefinition>
                            
                        </Grid.RowDefinitions>

                        
                        <!-- ZIMO_FUNCTIONKEYMAPPINGTYPE_CV61 -->
                        <VerticalStackLayout Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="10" Margin="0,10,0,20" IsVisible="{Binding ZIMO_FUNCTIONKEYMAPPINGTYPE_CV61}">
                            <Label LineBreakMode="WordWrap"  Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameFunctionsKeysZIMOMappingType}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <Label Text="{x:Static strings:AppResources.FrameFunctionsKeysZIMOMappingTypeLabel}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" TextColor="Red"> </Label>
                        </VerticalStackLayout>

                        <HorizontalStackLayout Grid.Row="1" Grid.Column="16" IsVisible="{Binding ZIMO_FUNCTIONKEYMAPPINGTYPE_CV61}"  HorizontalOptions="End">
                            <Label Text="{Binding Path=ZIMOExtendedFunctionMapping,Converter={StaticResource switchStateConverter}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" HorizontalOptions="End" VerticalOptions="Center" Margin="0,0,10,0" ></Label>
                            <Switch  Grid.ColumnSpan="2" HorizontalOptions="End" VerticalOptions="Center"  IsToggled="{Binding ZIMOExtendedFunctionMapping}"></Switch>
                        </HorizontalStackLayout>

                        <!-- DOEHLERANDHAASS_FUNCTIONKEYMAPPINGTYPE_CV137 -->
                        <VerticalStackLayout Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="10" Margin="0,10,0,20" IsVisible="{Binding DOEHLERANDHAASS_FUNCTIONKEYMAPPINGTYPE_CV137}" >
                            <Label Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameFunctionsKeysDoehlerAndHaassMappingType}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <Label Text="{x:Static strings:AppResources.FrameFunctionsKeysZIMOMappingTypeLabel}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" TextColor="Red" ></Label>
                        </VerticalStackLayout>
                        <HorizontalStackLayout Grid.Row="1" Grid.Column="16" IsVisible="{Binding DOEHLERANDHAASS_FUNCTIONKEYMAPPINGTYPE_CV137}"  HorizontalOptions="End">
                            <Label Text="{Binding Path=DoehlerAndHaassExtendedFunctionMappingEnabled,Converter={StaticResource switchStateConverter}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" HorizontalOptions="End" VerticalOptions="Center" Margin="0,0,10,0" ></Label>
                            <Switch IsVisible="{Binding DOEHLERANDHAASS_FUNCTIONKEYMAPPINGTYPE_CV137}" Grid.ColumnSpan="2" HorizontalOptions="End" VerticalOptions="Center"  IsToggled="{Binding DoehlerAndHaassExtendedFunctionMappingEnabled}"></Switch>
                        </HorizontalStackLayout>

                        <!-- The description of the function mapping -->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}" Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="18" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingDescription}" VerticalOptions="Center"  HorizontalOptions="Start" Margin="0,10,0,20" />

                        <BoxView IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Column="0" Grid.Row="3" Grid.RowSpan="2" Grid.ColumnSpan="1"  Color="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" />
                        <BoxView IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Column="1" Grid.Row="3" Grid.RowSpan="2" Grid.ColumnSpan="1"  Color="{AppThemeBinding Light={StaticResource TableHeaderHighlight_Light}, Dark={StaticResource TableHeaderHighlight_Dark}}" />
                        <BoxView IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Column="2" Grid.Row="3" Grid.RowSpan="2" Grid.ColumnSpan="1"  Color="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" />
                        <BoxView IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Column="3" Grid.Row="3" Grid.RowSpan="2" Grid.ColumnSpan="14"  Color="{AppThemeBinding Light={StaticResource TableHeaderHighlight_Light}, Dark={StaticResource TableHeaderHighlight_Dark}}" />
                        <BoxView IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Column="17" Grid.Row="3" Grid.RowSpan="2" Grid.ColumnSpan="1" Color="{AppThemeBinding Light={StaticResource TableHeaderNormal_Light}, Dark={StaticResource TableHeaderNormal_Dark}}" />


                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="3" Grid.Column="0" Grid.RowSpan="2" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Margin="5,2,5,2"   Text="{x:Static strings:AppResources.FrameFunctionKeysMappingHeaderFunctionkey}"  VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="3" Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="1" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Margin="5,2,5,2" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingHeaderDirection}" VerticalOptions="Center"  HorizontalOptions="Center"/>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="3" Grid.Column="2" Grid.RowSpan="2" Grid.ColumnSpan="1" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Margin="5,2,5,2" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingHeaderCV}"  VerticalOptions="Center"  HorizontalOptions="Center"/>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="3" Grid.Column="3"  Grid.ColumnSpan="14" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Margin="5,2,5,2" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingHeadeOutput}" VerticalOptions="Center"  HorizontalOptions="Center"/>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="3" Grid.Column="17" Grid.RowSpan="2" Grid.ColumnSpan="1" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Margin="5,2,5,2"  Text="{x:Static strings:AppResources.FrameFunctionKeysMappingHeadeValue}" VerticalOptions="Center"  HorizontalOptions="Center"/>

                        <!-- The header of the function mapping: The name of the different function outputs -->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="3" Text="{Binding Output0vDescription}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="4" Text="{Binding Output0rDescription}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="5" Text="{Binding Output1Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="6" Text="{Binding Output2Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="7" Text="{Binding Output3Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="8" Text="{Binding Output4Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="9" Text="{Binding Output5Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="10" Text="{Binding Output6Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="11" Text="{Binding Output7Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="12" Text="{Binding Output8Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="13" Text="{Binding Output9Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="14" Text="{Binding Output10Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="15" Text="{Binding Output11Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="4" Grid.Column="16" Text="{Binding Output12Description}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>

                        <!-- Key F0 foward mapping in CV33-->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="0" Text="F0" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="1" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingForward}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="2" Text="33" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="3" IsChecked="{Binding CV33ValueBit0}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="4" IsChecked="{Binding CV33ValueBit1}" VerticalOptions="Center"  HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="5" IsChecked="{Binding CV33ValueBit2}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="6" IsChecked="{Binding CV33ValueBit3}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="7" IsChecked="{Binding CV33ValueBit4}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="8" IsChecked="{Binding CV33ValueBit5}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="9" IsChecked="{Binding CV33ValueBit6}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="10" IsChecked="{Binding CV33ValueBit7}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"  Grid.Row="5" Grid.Column="17" Text="{Binding CV33Value}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>

                        <!-- Key F0 backward mapping in CV34-->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="0" Text="F0" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="1" Text="{x:Static strings:AppResources.FrameFunctionKeysMappingBackward}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="2" Text="34" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="3" IsChecked="{Binding CV34ValueBit0}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="4" IsChecked="{Binding CV34ValueBit1}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="5" IsChecked="{Binding CV34ValueBit2}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="6" IsChecked="{Binding CV34ValueBit3}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="7" IsChecked="{Binding CV34ValueBit4}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="8" IsChecked="{Binding CV34ValueBit5}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="9" IsChecked="{Binding CV34ValueBit6}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="10" IsChecked="{Binding CV34ValueBit7}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="6" Grid.Column="17" Text="{Binding CV34Value}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>


                        <!-- Key F1 mapping in CV35-->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="0" Text="F1" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="1" Text="-" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="2" Text="35" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="3" IsChecked="{Binding CV35ValueBit0}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="4" IsChecked="{Binding CV35ValueBit1}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="5" IsChecked="{Binding CV35ValueBit2}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="6" IsChecked="{Binding CV35ValueBit3}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="7" IsChecked="{Binding CV35ValueBit4}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="8" IsChecked="{Binding CV35ValueBit5}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="9" IsChecked="{Binding CV35ValueBit6}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="10" IsChecked="{Binding CV35ValueBit7}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="7" Grid.Column="17" Text="{Binding CV35Value}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>

                        <!-- Key F2 mapping in CV36-->
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="0" Text="F2" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="1" Text="-" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="2" Text="36" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="3" IsChecked="{Binding CV36ValueBit0}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="4" IsChecked="{Binding CV36ValueBit1}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="5" IsChecked="{Binding CV36ValueBit2}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="6" IsChecked="{Binding CV36ValueBit3}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="7" IsChecked="{Binding CV36ValueBit4}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="8" IsChecked="{Binding CV36ValueBit5}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="9" IsChecked="{Binding CV36ValueBit6}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <CheckBox IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="10" IsChecked="{Binding CV36ValueBit7}" VerticalOptions="Center" HorizontalOptions="Center"></CheckBox>
                        <Label IsEnabled="{Binding RCN225StandardFunctionMapping}"   Grid.Row="8" Grid.Column="17" Text="{Binding CV36Value}" VerticalOptions="Center" HorizontalOptions="Center" HorizontalTextAlignment="Center" ></Label>

