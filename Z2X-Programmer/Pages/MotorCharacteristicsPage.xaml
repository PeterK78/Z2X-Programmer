<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:converter="clr-namespace:Z2XProgrammer.Converter"
             xmlns:strings="clr-namespace:Z2XProgrammer.Resources.Strings"
             xmlns:viewmodel="clr-namespace:Z2XProgrammer.ViewModel"
             x:DataType="viewmodel:MotorCharacteristicsViewModel"
             x:Class="Z2XProgrammer.Pages.MotorCharacteristicsPage"
             BackgroundColor="{AppThemeBinding Light={StaticResource AppBackground_Light}, Dark={StaticResource AppBackground_Dark}}"
             Padding="40,40,5,20"
             Title="{x:Static strings:AppResources.FyoutItemMotorCharacteristicsText}">

    <ContentPage.Resources>
        <converter:InverseBooleanConverter x:Key="boolInvert" />
        <converter:SwitchStateToTextConverter x:Key="switchStateConverter" />
    </ContentPage.Resources>

    <ScrollView x:Name="PageScrollView" Orientation="Vertical" Padding="0,0,20,0">

        <VerticalStackLayout>

            <!-- Page Title -->
            <HorizontalStackLayout>
                <Image Source="{AppThemeBinding Light=ic_fluent_engine_24_regular.png, Dark=ic_fluent_engine_24_dark.png}" Margin="0,0,10,0" ></Image>
                <Label Style="{StaticResource Z2XProgrammerLabelHeading1Style}" Text="{x:Static strings:AppResources.FyoutItemMotorCharacteristicsText}" VerticalOptions="Center"  HorizontalOptions="Start" />
            </HorizontalStackLayout>

            <!-- No data available information -->
            <HorizontalStackLayout IsVisible="{Binding Path=DataStoreDataValid,Converter={StaticResource boolInvert}}" Margin="0,20,0,20" >
                <Image Source="{AppThemeBinding Light=ic_fluent_info_24_regular.png, Dark=ic_fluent_info_24_dark.png}" WidthRequest="24" Margin="0,0,20,0" ></Image>
                <Label Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.AlertNoInformationAvailable}" VerticalOptions="Center"  HorizontalOptions="Start" />
            </HorizontalStackLayout>

            <!-- Page sheet content -->
            <VerticalStackLayout IsVisible="{Binding DataStoreDataValid}">

                <!-- RCN225: Speed curve selection (standard or extended) in CV29 (RCN225_SPEEDTABLE_CV29_4) -->
                <Label Style="{StaticResource Z2XProgrammerLabelHeading2Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsCharacteristicCurveTypeTitle}" VerticalOptions="Center"  HorizontalOptions="Start" IsVisible="{Binding RCN225_SPEEDTABLE_CV29_4}" />
                <Border Style="{StaticResource Z2XBorderFrame}" IsVisible="{Binding RCN225_SPEEDTABLE_CV29_4}">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"></RowDefinition>
                            <RowDefinition Height="Auto"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,0,20" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsCharacteristicCurveTypeDescription}" VerticalOptions="Center"  HorizontalOptions="Start" />
                        <Label x:Name="FrameMotorCharacteristicsCharacteristicCurveTypeLabel" Grid.Row="1" Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsCharacteristicCurveTypeLabel}" VerticalOptions="Center"  HorizontalOptions="Start" />
                        <VerticalStackLayout Grid.Row="1" Grid.Column="0"  HorizontalOptions="End">
                            <HorizontalStackLayout>
                                <Label Text="{Binding Path=ExtendedSpeedCurveEnabled,Converter={StaticResource switchStateConverter}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" HorizontalOptions="End" VerticalOptions="Center" Margin="0,0,10,0" ></Label>
                                <Switch Style="{StaticResource Z2XProgrammerSwitchStyle}" IsToggled="{Binding ExtendedSpeedCurveEnabled}" VerticalOptions="Center" HorizontalOptions="End" ></Switch>
                            </HorizontalStackLayout>
                            <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV29Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                        </VerticalStackLayout>
                    </Grid>
                </Border>

                <!-- Basic speed curve with only 3 parameters (CV2, CV5 and CV6) -->
                <Label Style="{StaticResource Z2XProgrammerLabelHeading2Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveTitle}" VerticalOptions="Center"  HorizontalOptions="Start" IsVisible="{Binding Path=ExtendedSpeedCurveEnabled,Converter={StaticResource boolInvert}}" />
                <Border Style="{StaticResource Z2XBorderFrame}" IsVisible="{Binding Path=ExtendedSpeedCurveEnabled,Converter={StaticResource boolInvert}}">
                    <VerticalStackLayout>
                        <!-- RCN225: Minimum speed in CV2 -->
                        <Grid IsVisible="{Binding RCN225_MINIMALSPEED_CV2}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Label x:Name="FrameMotorCharacteristicsBasicCurveMinimumSpeedTitle" Grid.Row="0" Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMinimumSpeedTitle}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="0" Grid.Column="1">
                                <HorizontalStackLayout >
                                    <Slider Value="{Binding MinimumSpeed}" WidthRequest="300" Maximum="255" Minimum="1" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                                    <Label  Text="{Binding MinimumSpeedValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalOptions="End" HorizontalTextAlignment="End" ></Label>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV2Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>
                        </Grid>

                        <!-- PIKO SMARTDECODER: Minimum speed in CV2 (the upper limit is 63, not 255) -->
                        <Grid IsVisible="{Binding PIKOSMARTDECODER_MINIMUMSPEED_CV2}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Label x:Name="FrameMotorCharacteristicsPIKOBasicCurveMinimumSpeedTitle" Grid.Row="0" Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsPIKOBasicCurveMinimumSpeedTitle}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="0" Grid.Column="1">
                                <HorizontalStackLayout >
                                    <Slider Value="{Binding MinimumPikoSmartDecoderSpeed}" WidthRequest="300" Maximum="63" Minimum="1" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                                    <Label  Text="{Binding MinimumPikoSmartDecoderSpeedValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalOptions="End" HorizontalTextAlignment="End" ></Label>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV2Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>
                        </Grid>

                        <!-- RCN225: Medium speed in CV6 -->
                        <Grid Margin="0,20,0,0" IsVisible="{Binding RCN225_MEDIUMSPEED_CV6}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="FrameMotorCharacteristicsBasicCurveMediumSpeedTitle" Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMediumSpeedTitle}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="0" Grid.Column="1">
                                <HorizontalStackLayout>
                                    <Slider Value="{Binding MediumSpeed}" WidthRequest="300" Maximum="255" Minimum="1" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                                    <Label  Text="{Binding MediumSpeedValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalOptions="End" HorizontalTextAlignment="End" ></Label>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV6Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>
                        </Grid>

                        <!-- RCN225: Maximum speed CV5. The usage of CV5 can be enabled and disabled by setting CV5 to 0 or 1. See RCN-225 for more details -->
                        <Grid Margin="0,20,0,0" IsVisible="{Binding RCN225_MAXIMALSPEED_CV5}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>

                            <Label x:Name="FrameMotorCharacteristicsBasicCurveMaximumSpeedTitle" Grid.Row="0" Grid.Column="0"   Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSpeedTitle}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <Label Grid.Row="1" Grid.Column="0"   Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSpeedDefaultEnabled}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="1" Grid.Column="1"  HorizontalOptions="End">
                                <HorizontalStackLayout >
                                    <Label Text="{Binding Path=MaximumSpeedDefaultUsed,Converter={StaticResource switchStateConverter}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" HorizontalOptions="End" VerticalOptions="Center" Margin="0,0,10,0" ></Label>
                                    <Switch Style="{StaticResource Z2XProgrammerSwitchStyle}" IsToggled="{Binding MaximumSpeedDefaultUsed}" HorizontalOptions="End" ToolTipProperties.Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSwitchTooltip}"></Switch>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV5Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>

                            <Label Grid.Row="2" Grid.Column="0"  IsVisible="{Binding Path=MaximumSpeedDefaultUsed,Converter={StaticResource boolInvert}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSpeedLabel}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <HorizontalStackLayout Grid.Row="2" Grid.Column="1" IsVisible="{Binding Path=MaximumSpeedDefaultUsed,Converter={StaticResource boolInvert}}" >
                                <Slider Value="{Binding MaximumSpeed}" WidthRequest="300" Minimum="0"  Maximum="255"  VerticalOptions="Center" ToolTipProperties.Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSliderTooltip}" Style="{StaticResource Z2XSlider}"></Slider>
                                <Label  Text="{Binding MaximumSpeedValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalTextAlignment="End"></Label>
                            </HorizontalStackLayout>

                        </Grid>

                        <!-- PIKO SMARTDECODER: Maximum speed in CV5 (the upper limit is 63, not 255) PIKOSMARTDECODER_MAXIMUMSPEED_CV5 -->
                        <Grid Margin="0,20,0,0" IsVisible="{Binding PIKOSMARTDECODER_MAXIMUMSPEED_CV5}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>

                            <Label Grid.Row="0" Grid.Column="0" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSpeedLabel}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="0" Grid.Column="1">
                                <HorizontalStackLayout >
                                    <Slider Value="{Binding MaximumPikoSmartDecoderSpeed}" WidthRequest="300" Maximum="63" Minimum="1" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                                    <Label  Text="{Binding MaximumPikoSmartDecoderSpeedValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalOptions="End" HorizontalTextAlignment="End" ></Label>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV2Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>
                        </Grid>

                        <!-- DOEHLER AND HAAS: Maximum speed in CV5 (DOEHLERANDHAASS_MAXIMALSPEED_CV5)-->
                        <Grid Margin="0,20,0,0" IsVisible="{Binding DOEHLERANDHAASS_MAXIMALSPEED_CV5}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                                <ColumnDefinition Width="Auto"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>

                            <Label x:Name="FrameMotorCharacteristicsDoehlerHaasMaxSpeedTitel" Grid.Row="0" Grid.Column="0"   Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsDoehlerHaasMaxSpeedTitel}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <Label Grid.Row="1" Grid.Column="0"   Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsDoehlerHaasMaxSpeedDefaultEnabled}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <VerticalStackLayout Grid.Row="1" Grid.Column="1"  HorizontalOptions="End">
                                <HorizontalStackLayout >
                                    <Label Text="{Binding Path=MaximumSpeedDoehlerAndHaasDefaultUsed,Converter={StaticResource switchStateConverter}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" HorizontalOptions="End" VerticalOptions="Center" Margin="0,0,10,0" ></Label>
                                    <Switch Style="{StaticResource Z2XProgrammerSwitchStyle}" IsToggled="{Binding MaximumSpeedDoehlerAndHaasDefaultUsed}" HorizontalOptions="End" ToolTipProperties.Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSwitchTooltip}"></Switch>
                                </HorizontalStackLayout>
                                <Label IsVisible="{Binding AdditionalDisplayOfCVValues}" Text="{Binding CV5Configuration}" Style="{StaticResource Z2XProgrammerLabelAdditionalCVValuesStyle}" HorizontalOptions="Center" HorizontalTextAlignment="Center"></Label>
                            </VerticalStackLayout>

                            <Label x:Name="FrameMotorCharacteristicsDoehlerHaasMaxSpeedLabel" Grid.Row="2" Grid.Column="0"  IsVisible="{Binding Path=MaximumSpeedDoehlerAndHaasDefaultUsed,Converter={StaticResource boolInvert}}" Style="{StaticResource Z2XProgrammerLabelStandardStyle}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsDoehlerHaasMaxSpeedLabel}" VerticalOptions="Center"  HorizontalOptions="Start" />
                            <HorizontalStackLayout Grid.Row="2" Grid.Column="1" IsVisible="{Binding Path=MaximumSpeedDoehlerAndHaasDefaultUsed,Converter={StaticResource boolInvert}}" >
                                <Slider Value="{Binding MaximumDoehlerAndHaasSpeed}" WidthRequest="300" Minimum="0"  Maximum="127"  VerticalOptions="Center" ToolTipProperties.Text="{x:Static strings:AppResources.FrameMotorCharacteristicsBasicCurveMaximumSliderTooltip}" Style="{StaticResource Z2XSlider}"></Slider>
                                <Label  Text="{Binding MaximumSpeedDoehlerAndHaasValueDescription}" VerticalOptions="Center" Margin="10,0,0,0" MinimumWidthRequest="100" HorizontalTextAlignment="End"></Label>
                            </HorizontalStackLayout>

                        </Grid>


                    </VerticalStackLayout>
                </Border>

                <!-- RCN225: Extended speed curve in CV67 to CV94 -->
                <Label Style="{StaticResource Z2XProgrammerLabelHeading2Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurve}" VerticalOptions="Center"  HorizontalOptions="Start" IsVisible="{Binding ExtendedSpeedCurveEnabled}" IsEnabled="{Binding RCN225_EXTENDEDSPEEDCURVEVALUES_CV67X}" />
                <Border Style="{StaticResource Z2XBorderFrame}" IsVisible="{Binding ExtendedSpeedCurveEnabled}" IsEnabled="{Binding RCN225_EXTENDEDSPEEDCURVEVALUES_CV67X}">
                    <VerticalStackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" Rotation="270">

                        <!-- RCN225: Extended speed curve in CV67 -->
                        <HorizontalStackLayout>
                            <Label  x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep1CV67" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep1CV67}" VerticalOptions="Center" WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue67}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}" ></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue67}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV68 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep2CV68" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep2CV68}" VerticalOptions="Center" WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue68}"  Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue68}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV69 -->
                        <HorizontalStackLayout>
                            <Label  x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep3CV69" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep3CV69}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue69}"  Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue69}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV70 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep4CV70" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep4CV70}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue70}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue70}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV71 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep5CV71" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep5CV71}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue71}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue71}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV72 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep6CV72" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep6CV72}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue72}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue72}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV73 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep7CV73" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep7CV73}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue73}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue73}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV74 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep8CV74" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep8CV74}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue74}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}" ></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue74}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV75 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep9CV75" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep9CV75}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue75}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue75}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV76 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep10CV76" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep10CV76}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue76}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue76}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV77 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep11CV77" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep11CV77}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue77}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue77}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV78 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep12CV78" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep12CV78}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue78}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue78}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV79 -->
                        <HorizontalStackLayout>
                            <Label x:Name="FrameMotorCharacteristicsExtendedSpeedCurveStep13CV79" Style="{StaticResource Z2XProgrammerLabelHeading3Style}" Text="{x:Static strings:AppResources.FrameMotorCharacteristicsExtendedSpeedCurveStep13CV79}" VerticalOptions="Center"  WidthRequest="200"></Label>
                            <Slider Margin="40,0,0,0" Value="{Binding ExtendedSpeedCurveValue79}" Minimum="0" Maximum="255" HorizontalOptions="CenterAndExpand" WidthRequest="300" VerticalOptions="Center" Style="{StaticResource Z2XSlider}"></Slider>
                            <Label  Margin="40,0,0,0" Text="{Binding ExtendedSpeedCurveValue79}" VerticalOptions="Center" HorizontalOptions="End" HorizontalTextAlignment="End" WidthRequest="100" ></Label>
                        </HorizontalStackLayout>

                        <!-- RCN225: Extended speed curve in CV80 -->
                        <HorizontalStackLayout>
